{
	"name": "jest-zod-matchers",
	"version": "0.0.4",
	"description": "Custom Jest matchers to help validate Zod schemas and to prevent having a lot of boilerplate",
	"author": "Harm (https://github.com/harm-less)",
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"homepage": "https://github.com/harm-less/jest-zod-matchers",
	"bugs": "https://github.com/harm-less/jest-zod-matchers/issues",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/harm-less/jest-zod-matchers.git"
	},
	"keywords": [
		"javascript",
		"typescript",
		"jest",
		"zod",
		"matchers",
		"matcher",
		"tools",
		"testing"
	],
	"license": "MIT",
	"scripts": {
		"check:format": "prettier --list-different \"**/*.{js,ts}\"",
		"check:types": "tsc --noEmit",
		"compile": "tsc -p tsconfig.compile.json",
		"format": "prettier --write \"**/*.{js,ts}\"",
		"lint": "eslint",
		"test": "jest",
		"test:coverage": "npm run test -- --coverage"
	},
	"peerDependencies": {
		"@types/jest": ">=25.0.0",
		"jest": ">=25.0.0",
		"zod": ">=3.4.0",
		"zod-validation-error": ">=1.3.0"
	},
	"devDependencies": {
		"@eslint/eslintrc": "3.3.0",
		"@eslint/js": "9.21.0",
		"@types/jest": "29.5.14",
		"@typescript-eslint/eslint-plugin": "8.25.0",
		"@typescript-eslint/parser": "8.25.0",
		"eslint": "9.21.0",
		"eslint-config-prettier": "10.0.2",
		"jest": "29.7.0",
		"prettier": "3.5.2",
		"ts-jest": "29.2.6",
		"ts-node": "10.9.2",
		"typescript": "5.7.3",
		"zod": "3.24.2",
		"zod-validation-error": "3.4.0"
	}
}
